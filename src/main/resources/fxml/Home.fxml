                  <TextField fx:id="pathEntrada" prefHeight="31.0" prefWidth="292.0" style="-fx-background-color: #02030A; -fx-border-color: #B7C3D7; -fx-border-radius: 2em; -fx-text-fill: #ffffff; -fx-font-weight: bold;">
